{
  "name": "volakadu-website",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:nocheck": "vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rm -rf dist",
    "tailwind:build": "tailwindcss -i ./src/assets/base.css -o ./src/index.css",
    "tailwind:watch": "tailwindcss -i ./src/assets/base.css -o ./src/index.css --watch"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "firebase": "9.6.10",
    "nodemailer": "^6.7.3",
    "tar-fs": "^2.1.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
